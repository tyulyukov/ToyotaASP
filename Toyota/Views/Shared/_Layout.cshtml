    <header class="p-3 bg-dark text-white">
        <div class="container">
            <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                    <img src="https://png.monster/wp-content/uploads/2021/03/pngwing.com-2-3691ef4e-700x476.png" style="width: 80px;" />
                </a>

                <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                    <li><a href="/" class="nav-link px-2 text-white">Home</a></li>
                    <li><a asp-controller="Home" asp-action="Privacy" class="nav-link px-2 text-white">Privacy</a></li>
                    <li><a asp-controller="Home" asp-action="Cars" class="nav-link px-2 text-white">Cars</a></li>
                </ul>

                <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
                    <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
                </form>

                <partial name="_LoginPartial" />
            </div>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>

        <footer class="border-top" style="padding-top: 3rem;">
            <div class="row">
                <div class="col-2">
                    <h5>Cars lineup</h5>
                    <ul id="cars-lineup" class="nav flex-column">
                        @*<li class="nav-item mb-2"><a href="/" class="nav-link p-0 text-muted">Home</a></li>*@
                    </ul>
                </div>

                <div class="col-2">
                    <h5>Navigation</h5>
                    <ul class="nav flex-column">
                        <li class="nav-item mb-2"><a href="/" class="nav-link p-0 text-muted">Home</a></li>
                        <li class="nav-item mb-2"><a asp-controller="Home" asp-action="Privacy" class="nav-link p-0 text-muted">Privacy</a></li>
                        <li class="nav-item mb-2"><a asp-controller="Home" asp-action="Cars" class="nav-link p-0 text-muted">Cars</a></li>
                    </ul>
                </div>

                <div class="col-4 offset-1">
                    <h5>Request a call (~30sec)</h5>
                    <partial name="_CallBackPartial" />
                </div>
            </div>

            <div style="padding-top: 1.5rem; margin-top: 1.5rem;" class="d-flex justify-content-between border-top">
                <p>© 2022 Toyota, Inc. All rights reserved.</p>
                <ul class="list-unstyled d-flex">
                    <li class="ms-3"><a class="link-dark" target="_blank" href="https://www.t.me/tyulyukoff"><svg class="bi" width="24" height="24"><use xlink:href="#telegram"></use></svg></a></li>
                    <li class="ms-3"><a class="link-dark" target="_blank" href="https://www.instagram.com/tyulyukoff"><svg class="bi" width="24" height="24"><use xlink:href="#instagram"></use></svg></a></li>
                </ul>
            </div>
        </footer>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script>
        function renderModelsLineup(models) {
            let lineup = document.getElementById("cars-lineup");
            lineup.innerHTML = "";

            models.map(model => {
                let li = document.createElement("li");
                li.className = "nav-item mb-2";

                let a = document.createElement("a");
                a.href = "/Home/Cars";
                a.className = "nav-link p-0 text-muted";
                a.innerText = model.name;

                li.appendChild(a);
                lineup.appendChild(li);
            });
        }

        function getAllModels() {
            fetch("/api/ApiModels")
                .then(res => {
                    if (res.status !== 200) {
                        console.error(res);
                        return;
                    }

                    return res.json();
                })
                .then(models => {
                    if (models != undefined)
                        renderModelsLineup(models);
                })
                .catch(err => { console.error(err); });
        }

        getAllModels();
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
